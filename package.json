{
  "name": "artemis-reactjs",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "concurrently": "3.0.0",
    "node-sass": "^3.10.1",
    "postcss-cli": "^2.6.0",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "history": "4.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "^4.0.0-2"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names 'webpack, sass, postcss' --prefix name 'npm run start' 'npm run sass:watch' 'npm run postcss:watch'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "sass": "node-sass --recursive --output src/css --source-map true --source-map-contents src/sass",
    "sass:watch": "node-sass --watch --recursive --output src/css --source-map true --source-map-contents src/sass",
    "postcss": "postcss -u autoprefixer -o src/css/main.css src/css/main.css",
    "postcss:watch": "postcss -w -u autoprefixer -o src/css/main.css src/css/main.css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
